--commas inline
--funcattributes prev-line
--header "{file}"
--ifdef no-indent
--importgrouping testable-last
--indent 4
--maxwidth 140
--self init-only
--semicolons never
--typeattributes prev-line
--varattributes same-line
--wrapcollections before-first
--wrapparameters before-first
--asynccapturing expect
--throwcapturing expect
--selfrequired expect

--enable blankLineAfterImports,blankLinesBetweenImports

--disable andOperator,redundantType,extensionAccessControl,redundantExtensionACL,wrapMultilineStatementBraces,preferKeyPath,trailingClosures
